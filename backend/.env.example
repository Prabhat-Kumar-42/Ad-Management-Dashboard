# .env.example

JWT_SECRET=your_jwt_secret_key
JWT_EXPIRES_IN=your_jwt_expires_in

# Database
# SUPABASE DETAILS
DB_PASSWORD=your_db_password
DATABASE_URL=your_database_url

# Direct connection to the database. Used for migrations
DIRECT_URL=your_direct_database_url

# Redis for BullMQ
REDIS_URL=your_redis_url

# Google OAuth and Ads API
GOOGLE_CLIENT_ID=your_google_client_id
GOOGLE_CLIENT_SECRET=your_google_client_secret
GOOGLE_CONNECT_CALLBACK_URL=your_google_connect_callback_url
GOOGLE_LOGIN_CALLBACK_URL=your_google_login_callback_url
GOOGLE_DEVELOPER_TOKEN=your_google_ads_developer_token

# Meta (Facebook) OAuth
META_CLIENT_ID=your_meta_client_id
META_CLIENT_SECRET=your_meta_client_secret
META_CONNECT_CALLBACK_URL=your_meta_connect_callback_url
META_LOGIN_CALLBACK_URL=your_meta_login_callback_url

# (Optional / future)
NODE_ENV=development
PORT=3000
LOG_LEVEL=info
EMAIL_SERVICE_API_KEY=your_email_service_key
ENCRYPTION_KEY=your_encryption_key